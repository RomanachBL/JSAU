<html>
  <head>
    <meta charset="UTF-8" />
    <title>JSAU</title>
    <link rel="stylesheet" href="/CSS/style.css" type="text/css">
  </head>

  <body>
    <table>
      <tr id="tr_a">
        <th>ID</th>
        <th>Nom</th>
        <th>Nombre</th>
        <th>Genre</th>
      </tr>
      <% for (x in data) { %>
        <% if (id == data[x].id) { %>
        <tr>
          <td><%= data[x].id %></td>
          <td><%= data[x].nom %></td>
          <td><%= data[x].nombre %></td>
          <td><%= data[x].genre %></td>
        </tr>
        <% } %>
      <% } %>
    </table>
    <br><br>
    <h3><a href="/" style="margin-left: 10px" > Cliquez pour retourner Ã  l'accueil </a></h3>
  </body>
</html>
